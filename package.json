		"eslint-plugin-vue": "^7.9.0",
		"jest": "^26.6.3",
		"jest-serializer-vue": "^2.0.2",
		"jest-transform-stub": "^2.0.0",
		"ts-jest": "^26.5.5",
		"ts-loader": "^8.0.2",
		"typescript": "^4.2.0",
		"vue-jest": "^5.0.0-alpha.7",
